                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">with T 
as 

(

SELECT 
	DRSPORT.DonorNbr AS 'TRDOOR'
	,T04.SourceCode AS 'TRNSRCCOD'
	,P04.[Description] AS  'TRNSRCDSC'
	,0 AS 'TRNBATCH'
	,isnull(SELA10.PledgeCode,'') AS 'TRNPLEDGE'
	,SELBank.BankCode AS 'TRNBANK'
	,' ' AS 'TRNGFTDATE'
	,' ' AS 'TRNPRODATE'
	,0 AS 'TRNCOUNT'
	,SUM(T04.TotalAmount) 'TRNAMT'
	,MAX(format(T01.[Date],'MM/dd/yyyy')) AS 'DTRNGFTDTE'

	--,COALESCE(CONVERT(VARCHAR(10),D03.printdate,(101)), '') AS 'DTRNPRODTE' 	
	,'' AS 'DTRNPRODTE'
	, P04C.CodeValue AS 'TRNDEPT'
	,t01.DocumentNumber
--	INTO #TEMP 
from
tbl_FFPDRSPortfolios DRSPORT WITH (NOLOCK)
left outer join T01_TransactionMaster T01 WITH(NOLOCK) ON	T01.AccountNumber = DRSPORT.DonorNbr
inner join T04_GiftDetails T04 WITH(NOLOCK) ON T04.DocumentNumber = T01.DocumentNumber
--left outer join T12_TransactionResponseDetails T12 WITH(NOLOCK) ON T12.ResponseID = T04.ResponseID
--left outer join D03_ProductionProfiledGroupPrintGroups D03 WITH(NOLOCK) ON D03.PrintGroupId = T12.PrintGroupId 
join P04_SourceCodesMaster P04 WITH (NOLOCK) ON P04.SourceCode = T04.SourceCode
join P04cSourceCodes P04C WITH (NOLOCK) ON P04.SourceCode = P04C.SourceCode 
outer apply
   (SELECT top 1 B03.BankCode
	 FROM T13_TransactionDeposits T13 WITH (NOLOCK)
	 JOIN B03_DepositMaster B03 WITH (NOLOCK)
	 ON B03.DepositId = T13.DepositId AND T13.TableRecordId = T04.RecordId
	 WHERE T13.TableId = 'T04'
	)SELBank
outer apply
	(select top 1
		A10.PledgeCode		
	 from A10_AccountPledges A10 with (NOLOCK)
	 where
	  A10.AccountNumber = T01.AccountNumber 
	 --and A10.SourceCode = T04.SourceCode
                     and A10.PledgeId = T04.PledgeId
	) SELA10

WHERE 
--T12.CommunicationType = 'RECEIPT' 
--AND T12.ProfileGroupId &gt; 1
--and
 P04C.CodeType = 'DEPCOD'
 --and DRSPORT.DonorNbr = '42394'
 --order by T01.[Date] desc 
GROUP BY DRSPORT.DonorNbr
,T04.SourceCode 
,P04.[Description] 
,SELA10.PledgeCode
,SELBank.BankCode
,P04C.CodeValue
,t01.DocumentNumber

 )
 SELECT TRDOOR
 ,TRNSRCCOD
 ,TRNSRCDSC
 ,TRNBATCH
 ,TRNPLEDGE
 ,TRNBANK
 ,TRNGFTDATE
 ,TRNPRODATE
 ,TRNCOUNT
 ,TRNAMT
 ,DTRNGFTDTE
 ,DTRNPRODTE
 ,TRNDEPT
 
 FROM T</property>
