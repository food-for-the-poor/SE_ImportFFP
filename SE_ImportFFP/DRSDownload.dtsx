                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
	A01.AccountNumber AS  DONORNBR, 
	isnull(FP07.[EventCode],'') AS  MEVENT,
	isnull(x04.Description,'') AS  WKNAME,
	LEFT(dbo.getformattednamebyaccountnumber(A01.AccountNumber),70)AS  DNAME,
	isnull(A03.AddressLine4,'') AS D2NAME,								--As per Frank 3/2/19
	isnull(A06.Salutation,'') AS  DSALUT,
	A03.AddressLine1 AS  DADD1,
	isnull(A03.AddressLine2,'') AS  DADD2,
	A03.City AS  DCITY,
	A03.[State] AS  DSTATE,
	left(A03.ZipPostal,5) AS  DZIP,
	CASE WHEN LEN(A03.ZipPostal)&gt;5 and A03.Country = 'USA' THEN right(A03.ZipPostal,4) ELSE '' END   AS  DZIP4,
	isnull(PhoneCodeSEL.AreaCode,'') AS  AREACODE,
	isnull(PhoneCodeSEL.PhoneNumber,'') AS  PHONE,
	isnull(A01c4.CodeValue,'A') AS  DMAILS,	
	isnull(A01c5.CodeValue,'') AS DRELIG,	
	--iif(DonorStats.[Date Last Gift] is not null, format(DonorStats.[Date Last Gift],'MM/dd/yyyy','en-US'),'') TDATE,
	COALESCE(CONVERT(VARCHAR(10),DonorStats.[Date Last Gift],(101)), '') AS TDATE,	
	isnull(DonorStats.[Amount Last Gift],0) AS  DALGFT,
	isnull(DonorStats.[Amount This Year],0) AS  DT_YTD,
	isnull(DonorStats.[# Gifts This Year],0) AS  DTNYTD,
	isnull(DonorStats.[Amount Last Year],0) AS  DT_LY,
	isnull(DonorStats.[# Gifts Last Year],0) AS  DTNLY,
	isnull(DonorStats.[Amount Prior Year 1],0) AS  DT_PY1,
	isnull(DonorStats.[# Gifts This Prior Year 1],0) AS DTNPY1,
	isnull(DonorStats.[Amount Prior Year 2],0) AS  DT_PY2,
	isnull(DonorStats.[# Gifts This Prior Year 2],0) AS  DTNPY2,
	isnull(DonorStats.[Amount Prior Year 3],0) AS  DT_PY3,
	isnull(DonorStats.[# Gifts This Prior Year 3],0) AS DTNPY3,
	isnull(DonorStats.[Amount LTD],0) AS  DT_LTD,
	--iif(DonorStats.[Date First Gift] is not null, format(DonorStats.[Date First Gift],'MM/dd/yyyy','en-US'),'') FDATE,
	COALESCE(CONVERT(VARCHAR(10),DonorStats.[Date First Gift],(101)), '') AS FDATE, 
	ISNULL(DonorStats.[# Gifts LTD],0) AS DTNLTD,
	'' AS  DPREM,  --	isnull(A01c9.CodeValue,'') DPREM,	As pre Frank do not bring down to DRS (multiple values)
	isnull(A01c10.CodeValue,'') AS DSRCCD,	
	isnull(DonorStats.[Amount Largest Gift],0) AS  DABGFT,
	--iif(DonorStats.[Date Largest Gift] is not null, format(DonorStats.[Date Largest Gift],'MM/dd/yyyy','en-US'),'') LDATE,
	COALESCE(CONVERT(VARCHAR(10),DonorStats.[Date Largest Gift], (101)) , '') AS LDATE,
	--iif(SELA01d2.LastContactDate is not null, format(SELA01d2.LastContactDate,'MM/dd/yyyy','en-US'),'') CDATE,
	COALESCE(CONVERT(VARCHAR(10),SELA01d2.LastContactDate,(101)), '') AS CDATE, 
	LETSENTOUT = (SELECT COALESCE(CONVERT(VARCHAR(10),MAX(dtsnd.DateLetSent),(101)),'') FROM FP07_Campaign dtsnd WHERE dtsnd.AccountNumber = A01.AccountNumber AND dtsnd.[EventCode] IN (SELECT EventCode FROM tbl_FFPEventCodes WHERE Active = '1') )  ,
	isnull(A01c2.CodeValue,'')  AS DORGAN,	
	isnull(A01c11.CodeValue,'') AS  DPHNCD,	
	''  DPHON2,							-- As per Framk we are bringing Primary phone only
	--iif(SELFstGftDte.DteFstGft is not null, format(SELFstGftDte.DteFstGft,'MM/dd/yyyy','en-US'),'') EDATE,
	COALESCE(CONVERT(VARCHAR(10),SELFstGftDte.DteFstGft, (101)),'') AS EDATE,
	''  DLEVNT,			-- Event		Not Needed
	isnull(FP07.LetterWeek,0) AS  MW_LET,	
	isnull(FP07.WeekYear,0) AS DRPWKYEAR,	
	A01h.AssignedToAccessorId  AS DWRKER,
	0  AS DMDWRK,		-- Planned Giving Worker		A per Frank, check with Rick . Invisible now on DRS
	isnull(DonorStats.[Amount First Gift],0) DAFGFT,
	--iif(DonorStats.[Date First Gift] is not null, format(DonorStats.[Date First Gift],'MM/dd/yyyy','en-US'),'') MDATE,
	COALESCE(CONVERT(VARCHAR(10),DonorStats.[Date First Gift],(101)), '') AS MDATE,
	isnull(A07.EmailAddress,'') AS DEEADD,
	isnull(A01c14.CodeValue,'') AS DESUPD,	
	isnull(A01b.NumberValue,0) AS DDYR,
	isnull(A01b2.NumberValue,0) AS DDMM,
	isnull(A01b3.NumberValue,0) AS DDDD,
	isnull(A01.LastName,'') AS NPLNAM,
	isnull(A01.FirstName,'') AS NPFNAM,
	isnull(A23.P2GScore1,'') AS DP2G1,
	isnull(A23.P2GScore2,'') AS DP2G2,
	0  AS DPDEC,	--Not Required in drs Ricardo is supposed to import WelthEngine
	0  AS DMDEC,	--Not Required in drs Ricardo is supposed to import WelthEngine
	0  AS DPRES,	--Not Required in drs Ricardo is supposed to import WelthEngine
	0  AS DMRES,
	'' AS DBSR, --Ricardo is supposed to import WelthEngine
	'' AS DACAP, --Ricardo is supposed to import WelthEngine
	'' AS DDTYPE,	--It was not downloaded from DONOR400
	isnull(FP07.OutcomeCode,'') AS MCMPCD,
	isnull(SELRelName.RELNAME,'') AS RELIGDESC,
--	isnull(SELPremDesc.PremDesc,'') PREMDESC,
	isnull(left(SELPremDesc.PremDesc,30),'') AS 'PREMDESC',
	'' AS THANKYOU,		--It was not downloaded from DONOR400
	'' AS TIMEZONE,		-- Time Zone		Pending Ricardo Input
	'' AS RELATFLG,		-- Flag not required as relational data in Donor Direct 
	'' AS DMWRKNAM,		-- Not Required in drs 
	'' AS LSTMLCHG,		-- Check with Ricardo
	--iif(FP07.DateLetSent is not null, format(FP07.DateLetSent,'MM/dd/yyyy','es-US'),'') DRMDTLET,	
	COALESCE(CONVERT(VARCHAR(10),FP07.DateLetSent,(101)),'') AS DRMDTLET,
	isnull(FP07.SourceCode,'') AS  DRMSRCCD,
	isnull(FP07.CallWeek,0) AS  DRMWNCAL,
	isnull(FP07.FollowUpWeek,0) AS  DRMWNFUP,
	DRWKWTP = case
				when A01h.AssignmentType = 'SDA' then 'H'
				when A01h.AssignmentType = 'DA' then 'P'
				when A01h.AssignmentType = 'SCA' then 'M'
				else ''
				end,
	DRWKIGLC = case
				when A01h.AssignmentType = 'SDA' then '1'
				when A01h.AssignmentType = 'DA' then '2'
				when A01h.AssignmentType = 'SCA' then '3'
				when A01h.AssignedToAccessorId = 1158 then '6'
				when A01h.AssignedToAccessorId = 1339 then '5'
				else ''
				end,
	0 AS DRMJSDBM,			--Supposed to create a new table for major file data
	0 AS DRMJSDBD,			--Supposed to create a new table for major file data
	0 AS DRMJSDBY,			--Supposed to create a new table for major file data
	'' AS DRMJEMPN,			--Supposed to create a new table for major file data
	'' AS DRMJBTIT,			--Supposed to create a new table for major file data
	'' AS DRMJBPHN,			--Supposed to create a new table for major file data
	'' AS DRMJDECM,			--Supposed to create a new table for major file data
	'' AS DRMJEMPS,			--Supposed to create a new table for major file data
	'' AS DPHNUP,
	--iif(FP07.DateFollowUp is not null, format(FP07.DateFollowUp,'MM/dd/yyyy','en-US'),'') DRMDTFUP,	
	COALESCE(CONVERT(VARCHAR(10),FP07.DateFollowUp,(101)),'') AS DRMDTFUP,	
	isnull(A01c12.CodeValue,'') AS DMXFLG,	
	isnull(A01c13.CodeValue,'') AS DHXFLG,	
	isnull(A01c3.CodeValue,'A') AS DSOLIC,	
	'' AS APPROVED,
	'' AS CURRCHANGE,
	0 AS NEWWORKER,
	0 AS NEWDROPWK,
	0 AS NEWDRPWKYR,
	'' AS PRTUPDATED,
	'' AS PORTTIER,
	'' AS DRLSTCALL,
	'' AS REQREMOVAL,
	'' AS SENDFLYER,
	'' AS PORTCHANGE,
	'' AS PORTAOHITM
	
from A01_AccountMaster A01 (NOLOCK)
left join FP07_Campaign FP07 (NOLOCK)
on FP07.[AccountNumber] = A01.AccountNumber and FP07.[EventCode] in (SELECT CampEvent FROM tbl_FFPEventCodes WHERE Active = '1')
join A01hAccountAssignments A01h (NOLOCK)
on A01.AccountNumber = A01h.AccountNumber
join A02_AccountAddresses A02 with (NOLOCK)	
on A02.AccountNumber = A01.AccountNumber and A02.UseAsPrimary = 1
join A03_AddressMaster A03 with (NOLOCK)	
on A03.AddressID = A02.AddressID
left join A06_AccountSalutations A06 with (NOLOCK)	
on A06.AccountNumber = A01.AccountNumber AND A06.UseAsPrimary = 1
left join A07_AccountEmails A07 with (NOLOCK)	
on A07.AccountNumber = A01.AccountNumber AND A07.UseAsPrimary = 1
left join A01cAccountCodes A01c3 with (NOLOCK)	
on A01.AccountNumber = A01C3.AccountNumber and A01C3.CodeType = 'LANGUAGE'
left join A01cAccountCodes A01c2 with (NOLOCK)	
on A01.AccountNumber = A01C2.AccountNumber and A01C2.CodeType = 'ORGTYPE' and A01c2.Active = 1
left join A01cAccountCodes A01c4 with (NOLOCK)	
on A01.AccountNumber = A01C4.AccountNumber and A01C4.CodeType = 'MAILSTAT'
left join A01cAccountCodes A01c5 with (NOLOCK)	
on A01.AccountNumber = A01C5.AccountNumber and A01C5.CodeType = 'RELCOD'
--left join A01cAccountCodes A01c9 with (NOLOCK)	
--on A01.AccountNumber = A01C9.AccountNumber and A01C9.CodeType = 'PREMREF'
left join A01cAccountCodes A01c10 with (NOLOCK)	
on A01.AccountNumber = A01C10.AccountNumber and A01C10.CodeType = 'DDCSRCCODE'
left join A01cAccountCodes A01c11 with (NOLOCK)	
on A01.AccountNumber = A01C11.AccountNumber and A01C11.CodeType = 'PHNCOD'
left join A01cAccountCodes A01c12 with (NOLOCK)	
on A01.AccountNumber = A01C12.AccountNumber and A01C12.CodeType = 'MEXCFLAG'
left join A01cAccountCodes A01c13 with (NOLOCK)	
on A01.AccountNumber = A01C13.AccountNumber and A01C13.CodeType = 'HEXCFLAG'
left join A01cAccountCodes A01c14 with (NOLOCK)	
on A01.AccountNumber = A01C14.AccountNumber and A01C14.CodeType = 'EMLPREF'
left join A01bAccountNumbers A01b (nolock)
on A01b.AccountNumber = A01.AccountNumber and A01b.NumberType = 'BIRTHYEAR'
	and A01b.Active = 1
left join A01bAccountNumbers A01b2 (nolock)
on A01b2.AccountNumber = A01.AccountNumber and A01b2.NumberType = 'BIRTHMONTH'
	and A01b2.Active = 1
left join A01bAccountNumbers A01b3 (nolock)
on A01b3.AccountNumber = A01.AccountNumber and A01b3.NumberType = 'BIRTHDAY'
	and A01b3.Active = 1
left Join X04_CODEVALUES X04 WITH (NOLOCK)
ON convert(bigint,X04.VALUE) = A01h.AssignedToAccessorId and X04.Type = 'SRCWRK'
left join A23_AccountWealthEngine A23 (nolock)
on A23.AccountNumber = A01.AccountNumber
outer apply(
    select top 1 DONSTATS.[Date First Gift] as 'DteFstGft'
    from tbl_FFPDonorStats DONSTATS (nolock)
    Where DonStats.AccountNumber = A01.AccountNumber
    ) SELFstGftDte
outer apply (
        select 
                A01d.DateValue as 'InceptionDate'
        from
                A01dAccountDates A01d WITH (NOLOCK)
        where
                A01d.AccountNumber = A01.AccountNumber and A01d.DateType = 'DDCINCEPT' and A01d.Active ='1'
        ) SELA01d
outer apply (
        select 
                MAX(A01d2.DateValue) as 'LastContactDate'
        from
                A01dAccountDates A01d2 WITH (NOLOCK)
        where
                A01d2.AccountNumber = A01.AccountNumber and A01d2.DateType = 'LASTCONT'
        ) SELA01d2
---- left join A01dAccountDates A01d on A01d.AccountNumber = A01.AccountNumber and  A01d.DateType = 'DDCINCEPT'
---- left join A01dAccountDates A01d2 on A01d.AccountNumber = A01.AccountNumber and  A01d2.DateType = 'LASTCONT'
OUTER APPLY (
        select top 1
                A04.AccountNumber
				,trim(A04.AreaCode) as 'AreaCode', trim(A04.TelephoneNumber) as 'PhoneNumber'
				,A01c6.CodeValue as 'PhoneCode'
		from
                A04_AccountPhones A04 WITH (NOLOCK)
        left outer join
                A01cAccountCodes A01c6 WITH (NOLOCK)
        on
                A01c6.AccountNumber = A04.AccountNumber and
                A01c6.CodeType = 'PHNCOD' and A04.Active = 1 --and A04.UseAsPrimary = 1
         where
                A04.AccountNumber = A01.AccountNumber and A04.Active = 1 and A04.UseAsPrimary = 1
				-- and A01c6.CodeValue not in('P','T')
        ) PhoneCodeSEL
OUTER APPLY (
        select top 1 X04.Description as 'RELNAME'
        from  A01cAccountCodes A01c8
		left join X04_CodeValues X04
		on x04.Type = A01c8.CodeType and X04.Value = A01c8.CodeValue
        where A01c8.AccountNumber = A01.AccountNumber and A01c8.CodeType = 'RELCOD'
        ) SELRelName
OUTER APPLY (
        select top 1 X04.Description as 'PREMDESC'
        from  A01cAccountCodes A01c15
		left join X04_CodeValues X04
		on x04.Type = A01c15.CodeType and X04.Value = A01c15.CodeValue
        where A01c15.AccountNumber = A01.AccountNumber and A01c15.CodeType = 'PREMREF'
        ) SELPremDesc
left join tbl_FFPDonorStats DonorStats
on DonorStats.AccountNumber = A01.AccountNumber 
where A01h.FunctionalCategory = 'DR' and A01h.Active = '1' and A01h.[Primary] = '1'</property>
