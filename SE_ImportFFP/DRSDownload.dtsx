                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">with T 
as 

(
SELECT 
       DRSPORT.DonorNbr AS 'Donor'
       ,T04.PledgeID 'PledgeCode'
       ,sum(T04.TotalAmount) 'PaidAmount'
       ,Max(T01.Date) 'LastPaymentDate'
from
tbl_FFPDRSPortfolios DRSPORT WITH (NOLOCK)
left outer join T01_TransactionMaster T01 WITH(NOLOCK) ON     T01.AccountNumber = DRSPORT.DonorNbr
inner join T04_GiftDetails T04 WITH(NOLOCK) ON T04.DocumentNumber = T01.DocumentNumber
join P04_SourceCodesMaster P04 WITH (NOLOCK) ON P04.SourceCode = T04.SourceCode
WHERE 
  --DRSPORT.DonorNbr = @DonorNumber AND 
  T04.PledgeID &gt; ' '
group by DRSPORT.DonorNbr, T04.PledgeID
)
SELECT
              T.Donor 'Donor Number'
       , SELA10.PledgeType 'Pledge Type'
       , SELA10.PledgeCode 'Pledge Code'
       , COALESCE(CONVERT(VARCHAR(10),SELA10.StartDate,(101)), '') AS 'Beginning Date'   
       , COALESCE(CONVERT(VARCHAR(10),SELA10.EndDate,(101)), '') AS 'Ending Date' 
       , COALESCE(CONVERT(VARCHAR(10),T.LastPaymentDate,(101)), '') AS 'Last Pymt Date' 
       , T.PaidAmount 'Amount Paid'
       , iif(SELA10.PledgeType = 'G',0,SELA10.TotalAmount) 'Total Pledge'
       , SELA10.AmountPerGift
       , iif(SELA10.PledgeType = 'P', SELA10.TotalAmount - T.PaidAmount,0) 'Remaining Amount'
--     , iif(SELA10.PledgeType = 'P', SELA10.AmountPerGift * SELA10.NumberPledged - T.PaidAmount,0) 'Remaining Amount'
       , SELA10.PledgeFrequency 'Bill Cycle'
       , SELA10.PledgeStatus      

FROM T
outer apply
       (select top 1
              A10.PledgeCode, A10.TotalAmount, A10.AmountPerGift, A10.PledgeFrequency
              ,A10.NumberPledged, A10.StartDate, A10.EndDate, iif(A10.OpenEnded = 1,'G','P') as 'PledgeType'         
              ,A10.PledgeStatus
       from A10_AccountPledges A10 with (NOLOCK)
       where
         A10.AccountNumber = T.Donor and A10.PledgeId = T.PledgeCode
       ) SELA10
order by SELA10.EndDate</property>
