	 AND X04.[Value] = A05.CommunicationType
	) SELTypeDesc
WHERE 
--A05.LongComment IS NOT NULL AND 
A05.[Date] IS NOT NULL 
AND A01h.FunctionalCategory = 'DR' 
AND A01h.Active = 1 
AND A01h.[Primary] = 1 
AND A01.FamilyiD ='0' 

