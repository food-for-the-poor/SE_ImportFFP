    ,'"' + LEFT(Replace(Replace(Replace(Replace([dbo].[udf_ffp_StripHTML](A05.LongComment),CHAR(10),''),CHAR(13),''),CHAR(34),''),CHAR(150),''),254) +'"'   AS CCOMNT
	,ISNULL(A05.[Status],'') 'CCNTYP'
	,ISNULL(A05.CommunicationType,'') CCTYPE
	, SELTypeDesc.[Description] AS COMTDESC
	,' ' AS COMTAMT
	,ISNULL(FP30.AccessorDescription,'') CUSER
	, CONVERT(VARCHAR(10),A05.[Date],(101)) AS COMTDATE


FROM  tbl_FFPDRSPortfolios DRSPORT WITH (NOLOCK)
