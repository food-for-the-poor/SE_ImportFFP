AS

(

--Individual record 

SELECT A05.AccountNumber as AccountNumber
, A05.AssignedToAccessorId
, max(A05d.DateValue) as 'DRDATE'
FROM A05_AccountCommunications A05 with (nolock)
INNER JOIN  A05dAccountCommunicationDates A05d with (nolock) on A05d.CommunicationRecordId = A05.RecordId 
INNER JOIN A01_AccountMaster A01 with(nolock) on a05.AccountNumber = A01.AccountNumber

WHERE  A05d.DateType = 'DRDATE'
AND A01.FamilyiD ='0' 

GROUP BY A05.AccountNumber, A05.AssignedToAccessorId

UNION ALL 

--Main group record 

SELECT A05a.AccountNumber as AccountNumber
, A05a.AssignedToAccessorId
, max(A05d1.DateValue) as 'DRDATE'
FROM A05_AccountCommunications A05a with (nolock)
INNER JOIN  A05dAccountCommunicationDates A05d1 with (nolock) on A05d1.CommunicationRecordId = A05a.RecordId 
INNER JOIN A01_AccountMaster A01 with(nolock) on a05a.AccountNumber = A01.AccountNumber

WHERE  A05d1.DateType = 'DRDATE'
AND A01.FamilyiD =A01.AccountNumber

GROUP BY A05a.AccountNumber, A05a.AssignedToAccessorId

UNION ALL 

--Individual group record 

SELECT A01.FamilyId as AccountNumber
, A05b.AssignedToAccessorId
, max(A05d2.DateValue) as 'DRDATE'
FROM A05_AccountCommunications A05b with (nolock)
INNER JOIN  A05dAccountCommunicationDates A05d2 with (nolock) on A05d2.CommunicationRecordId = A05b.RecordId 
INNER JOIN A01_AccountMaster A01 with(nolock) on a05b.AccountNumber = A01.AccountNumber

WHERE  A05d2.DateType = 'DRDATE'
AND A01.FamilyMemberType IN ('SP','CN','DP')

GROUP BY A01.FamilyId, A05b.AssignedToAccessorId

) 


