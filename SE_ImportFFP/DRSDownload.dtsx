	, SELTypeDesc.[Description] AS COMTDESC
	,' ' AS COMTAMT
	,ISNULL(FP30.AccessorDescription,'') CUSER
	, CONVERT(VARCHAR(10),A05.[Date],(101)) AS COMTDATE


FROM  tbl_FFPDRSPortfolios DRSPORT WITH (NOLOCK)
INNER JOIN A01_AccountMaster A01 WITH (NOLOCK) ON  DRSPORT.DONORNBR = A01.AccountNumber
LEFT OUTER JOIN A01hAccountAssignments A01h WITH (NOLOCK) on A01h.AccountNumber = A01.AccountNumber
LEFT OUTER JOIN A05_AccountCommunications A05 WITH (NOLOCK) on A05.AccountNumber = A01.AccountNumber

