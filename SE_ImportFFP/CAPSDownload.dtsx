              description="OLE DB Source"
              name="OLE DB Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH SRCDownload AS (
SELECT 
	P04c.SourceCode 
   , '"'+P04.[Description]+'"' AS 'Description' 
   ,isnull(T04.TotalAmount,0) AS 'GiftAmount'
   , isnull(T01.[Date],'') AS 'Last Gift Date'	
FROM P04cSourceCodes P04c (NOLOCK)
LEFT JOIN T04_GiftDetails T04 (NOLOCK) ON T04.SourceCode = P04c.SourceCode 
LEFT JOIN T01_TransactionMaster T01 (NOLOCK) ON T01.DocumentNumber = T04.DocumentNumber
JOIN P04_SourceCodesMaster P04 (NOLOCK) ON P04.SourceCode = P04c.SourceCode
OUTER APPLY
       (SELECT TOP 1 P04c2.CodeValue                                                            /* SELECT SOURCE CODE WORKER */
		FROM P04cSourceCodes P04c2 WITH (NOLOCK) 
		WHERE P04c2.SourceCode = P04c.SourceCode and CodeType =  'GFTCOD'
       ) SEL1
WHERE P04c.CodeType = 'DEPCOD' 
  AND P04c.CodeValue IN('30.000','65.000')
  AND SEL1.CodeValue NOT IN('PA', 'HO', 'EX', 'SJ', 'CH')
)
SELECT 
	  SourceCode AS 'Source Code'
	, SRCDownload.[Description]
	, sum(CONVERT(DECIMAL(10,2),SRCDownload.GiftAmount)) AS 'LTD Amount'
	, count(SRCDownload.GiftAmount) AS 'LTD #Gifts'
	, format(max(SRCDownload.[Last Gift Date]),'MM/dd/yyyy','en-US') AS [Last Gift Date]
FROM
	SRCDownload
GROUP BY SRCDownload.SourceCode, SRCDownload.[Description]
--HAVING count(CONVERT(INT,SRCDownload.GiftAmount)) &gt;1
ORDER BY SRCDownload.SourceCode</property>
