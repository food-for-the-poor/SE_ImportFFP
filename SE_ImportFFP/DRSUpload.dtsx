              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT TOP  100
	DRSPORT.DONORNBR CDONOR
	,FORMAT(A05.[Date],'MM/dd/yyyy','EN-US') AS  CCOMDT
	,'"' + A05.LongComment +'"'   AS CCOMNT
	,ISNULL(A05.[Status],'') 'CCNTYP'
	,ISNULL(A05.CommunicationType,'') CCTYPE
	, SELTypeDesc.[Description] AS CTDESC
	,' ' AS CGFAMT
	,ISNULL(FP30.WorkerName,'') CUSER
	, A05.AssignedToAccessorId

FROM  tbl_FFPDRSPortfolios DRSPORT WITH (NOLOCK)
LEFT JOIN A01hAccountAssignments A01h WITH (NOLOCK) on A01h.AccountNumber = DRSPORT.DONORNBR and A01h.FunctionalCategory = 'DR'
LEFT JOIN A05_AccountCommunications A05 WITH (NOLOCK) on A05.AccountNumber = DRSPORT.DONORNBR
LEFT JOIN FP30_WorkerIdXref FP30 WITH (NOLOCK) on FP30.WorkerId = A05.AssignedToAccessorId

OUTER APPLY
	(SELECT TOP 1 [Description]
	 FROM X04_COdeValues X04 WITH (NOLOCK)
	 WHERE X04.[Type] = 'DDCCOMTYPE' 
	 AND X04.[Value] = A05.CommunicationType
	) SELTypeDesc

ORDER BY A05.[Date] DESC</property>
