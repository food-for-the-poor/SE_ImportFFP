      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C3B22316-AF3C-4D9F-8CD4-E8570E49FD31}"
          SQLTask:SqlStatementSource="&#xA;&#xA;--CHECKING IF TEMP TABLES EXIST&#xA;&#xA;IF OBJECT_ID('tempdb..#colReNm') IS NOT NULL&#xA;    DROP TABLE #colReNm&#xA;&#xA;&#xA;IF OBJECT_ID('tempdb..#CodeVal') IS NOT NULL&#xA;    DROP TABLE #CodeVal&#xA;&#xA;&#xA;&#xA;&#xA;SET XACT_ABORT ON &#xA;&#xA;BEGIN TRANSACTION&#xA;&#xA;UPDATE P04_SourceCodesMaster&#xA;&#x9;SET [Description] = upd.[Description], StartDate = upd.[StartDate]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;FROM (SELECT fsu.SourceCode, fsu.[Description], fsu.[StartDate]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;FROM FFP_SourceUpdate fsu) upd&#xA;&#xA;&#x9;WHERE P04_SourceCodesMaster.SourceCode = upd.SourceCode&#xA;&#xA;&#xA;&#xA;--Renaming columns into temp table &#xA;&#xA;&#xA;SELECT SourceCode&#xA;, CodeValue_DEPCOD as DEPCOD&#xA;, CodeValue_GFTCOD as GFTCOD&#xA;, CodeValue_SRCWRK as SRCWRK &#xA;, CodeValue_MAILED# as MAILED&#xA;,CONVERT(VARCHAR(10),StartDate,(101)) AS StartDate&#xA;,[Posted On] as PostedOn&#xA;INTO #colReNm&#xA;FROM FFP_SourceUpdate&#xA;&#xA;--UnPivot data from column to row &#xA;&#xA;select cd.SourceCode, cd.CodeType, cd.CodeValue, cd.StartDate, cd.PostedOn&#xA;into #CodeVal&#xA;FROM #colReNm&#xA;unpivot &#xA;(CodeValue &#xA;for CodeType in (DEPCOD, GFTCOD, SRCWRK, MAILED)&#xA;&#xA;) cd&#xA;&#xA;&#xA;-- update P04cSourceCodes table &#xA;&#xA;UPDATE p4c&#xA;SET p4c.CodeValue = CASE WHEN p4c.CodeType = 'DEPCOD' and c.CodeType = 'DEPCOD' THEN c.CodeValue &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN p4c.CodeType = 'GFTCOD' and c.CodeType = 'GFTCOD' THEN c.CodeValue &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN p4c.CodeType = 'SRCWRK' and c.CodeType = 'SRCWRK' THEN c.CodeValue &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE p4c.CodeValue&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END &#xA;&#x9;&#xA;FROM P04cSourceCodes p4c&#xA;INNER JOIN #CodeVal c ON p4c.SourceCode = c.SourceCode and p4c.CodeType = c.CodeType &#xA;&#xA;&#xA;--update P04bSourceNumbers table &#xA;&#xA;UPDATE p4b&#xA;SET p4b.NumberValue = CASE WHEN  c.CodeValue = 'MAILED' THEN c.CodeValue ELSE p4b.NumberValue END &#xA;&#xA;/*test*/&#xA;--select p4b.NumberValue , CASE WHEN  c.CodeValue = 'MAILED' then c.CodeValue else p4b.NumberValue end , p4b.NumberType  &#xA;FROM P04bSourceNumbers p4b&#xA;INNER JOIN #CodeVal c ON p4b.SourceCode = c.SourceCode and p4b.NumberType = 'MAILED#'&#xA;&#xA;&#xA;INSERT INTO P04bSourceNumbers&#xA;&#x9;(&#xA;&#x9;SourceCode&#xA;&#x9;, NumberType&#xA;&#x9;, NumberValue&#xA;&#x9;, Active&#xA;&#x9;)&#xA;&#x9;&#xA;&#x9;SELECT DISTINCT fsu.SourceCode&#xA;&#x9;&#x9;&#x9;,'MAILED#'&#xA;&#x9;&#x9;&#x9;, CodeValue_MAILED#&#xA;&#x9;&#x9;&#x9;, '1'&#xA;&#x9;&#x9;FROM FFP_SourceUpdate fsu&#xA;&#x9;&#x9;&#x9;JOIN P04bSourceNumbers p04b ON fsu.SourceCode = p04b.SourceCode&#xA;&#x9;&#x9;&#x9;&#x9;WHERE p04b.SourceCode NOT IN (SELECT p04b.SourceCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM P04bSourceNumbers p04b &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;JOIN FFP_SourceUpdate fsu ON p04b.SourceCode = fsu.SourceCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; &#x9;&#x9;&#x9;&#x9;WHERE NumberType = 'MAILED#')&#xA;&#xA;&#xA;&#xA;COMMIT TRANSACTION ; &#xA;&#xA;DROP TABLE #colReNm; DROP TABLE #CodeVal &#xA;&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
